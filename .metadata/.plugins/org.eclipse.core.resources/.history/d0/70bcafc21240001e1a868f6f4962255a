package oop.inheritance;

public class A {
	int i;

	A() {
	}

	A(int i) {
		this.i = i;
	}

	void foo() {
		System.out.println("foo() from A");
	}
}

class B extends A {
	B() {
		super();
	}

	B(int i) {
		super(i);
	}
}

class TestInheritance{
	public static void main(String[] args) {
		B b = new B(5);
		b.foo();
	}
}
