package oop.inheritance;

public class TestUser {
	public static void main(String[] args) {
		User u1 = new User(101, "rahul");
		User u2 = new User(102, "sai");
		User u3 = new User(103, "surya");
		User u4 = new User(103, "ashok");
		User u5 = new User(104, "manish");
		
		u1.show();
		u2.show();
		u3.show();
		u4.show();
		u5.show();
	}
}
