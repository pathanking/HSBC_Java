package basics;

/**
 * 
 * @author razaa
 * @apiNote find max of three numbers using nested if-else
 */

public class NestedIfElse {

	public static void main(String[] args) {
		int a = 2, b = 5, c = 3;

		if (a > b) {
			if (a > c)
				System.out.printf("%d is max", a);
			else
				System.out.printf("%d is max", c);

		} else {
			if (b > c)
				if (b > a)
					System.out.printf("%d is max", b);
				else
					System.out.printf("%d is max", a);
		}

	}
}