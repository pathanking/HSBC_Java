package oop.inheritance;

public class Admin extends User {
	boolean access;
	String contact;

	Admin(int uid, String name, boolean access, String contact) {
		super(uid,name);
		this.access = access;
		this.contact = contact;
	}
	
	void show() {
		super.show();
		System.out.println("Access : "+access);
		System.out.println("Contact : "+contact);
	}

}
