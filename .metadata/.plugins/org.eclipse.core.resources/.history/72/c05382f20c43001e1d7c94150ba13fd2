package multi_threads;

public class ThreadDemo3 {
	public static void main(String[] args) {
		Thread t1 = new Thread(() -> {
			for (int i = 0; i < 10; i++)
				System.out.println(Thread.currentThread().getName() + " : " + i);
		});

		Thread t2 = new Thread(() -> {
			for (int i = 90; i < 100; i++)
				System.out.println(Thread.currentThread().getName() + " : " + i);
		});
		
		t1.start();
		t2.start();
	}
}
